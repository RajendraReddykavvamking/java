import java.util.Scanner;
public class Sum
{
    public static void main(String[] args)
    {
        Scanner sc= new Scanner(System.in);
        int n=sc.nextInt();
        int temp,sum=0,digit;
        temp=n;
        while(n>0)
        {
            digit=n%10;
            sum=sum+digit;
            n=n/10;
    }
    System.out.println(sum);
    }
}
